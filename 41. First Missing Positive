class Solution {
public:
    int firstMissingPositive(vector<int>& nums) {
        int res=nums.size();
        vector<int> nums1;
        for(int i=0;i<nums.size();i++){
            if(nums[i]>=0){
                nums1.push_back(nums[i]);
            }
        }
        sort(nums1.begin(),nums1.end());
        int target=1;
        for(int n:nums1){
            if(n==target)target++;
            else if(n>target) return target;
        }
        return target;;
    }
};
